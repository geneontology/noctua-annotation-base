  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4202",
    "clean": "rm -r workbenches/noctua-form/public",
    "build": "ng build --prod --index src/inject.tmpl  --base-href  /workbench/noctua-form/ ",
    "build:beta-test": "ng build --prod --index src/inject.tmpl --output-path workbenches/noctua-form-beta/public --base-href  /workbench/noctua-form-beta/ --configuration=beta-test",
    "start-hmr": "ng serve --configuration hmr -sm=false",
    "start-hmr-sourcemaps": "ng serve --hmr -e=hmr",
    "build-dev": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev",
    "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
    "build-prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "typedoc": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --out docs/typedoc src/app",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "packagr": "ng-packagr -p ng-package.json"
  },
